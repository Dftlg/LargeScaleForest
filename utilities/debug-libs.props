<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <GLUT_LIBRARIES>C:\lib2015\distrib\lib\Debug\freeglutd.lib</GLUT_LIBRARIES>
    <GLUI_LIBRARIES>$(SolutionDir)\libraries\glui\glui-2.35\src\msvc\lib\Debug\GLUI.lib</GLUI_LIBRARIES>
    <TBB_LIBRARIES>
    </TBB_LIBRARIES>
    <TIFF_LIBRARIES>
    </TIFF_LIBRARIES>
    <JPEG_LIBRARIES>
    </JPEG_LIBRARIES>
    <PNG_LIBRARIES>
    </PNG_LIBRARIES>
    <GLEW_LIBRARIES>
    </GLEW_LIBRARIES>
  </PropertyGroup>
  <PropertyGroup>
    <LibraryPath>$(IntelSWTools)\mkl\lib\intel64_win;$(IntelSWTools)\compiler\lib\intel64_win;$(Pre-built)\lib\x64;$(CGAL)\auxiliary\gmp\lib;$(Glui)\msvc\lib\Debug;$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link />
    <Link />
    <Link>
